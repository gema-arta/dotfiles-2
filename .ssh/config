Host devio.us
    User            wcmaier

Host openbsd
    HostName        cvs.openbsd.org
    User            wcmaier
    ForwardAgent    no
    LocalForward    7326 localhost:7326

Host meta
    HostName        meta.lfod.us
    RemoteForward   0 localhost:22
    SendEnv         SSH_*
    #RequestTTY     force

Host cloud
    HostName        cloud.lfod.us

Host *.lfod.us
    User            will

Host *
    Compression     yes
    ConnectTimeout  5
    ControlPath     ~/.ssh/controls/ctl-%r-%h-%p
    ControlMaster   auto
    #ControlPersist  1h
    ForwardAgent    yes
    ForwardX11      no
    Protocol        2
    EscapeChar      !
    HashKnownHosts  yes
    ServerAliveInterval 30
    User            will
