Host whilp
     Hostname 10.168.53.34

Host github.banksimple.com github.com github
     ProxyCommand none

Host *.banksimple.com
     ProxyCommand ssh -W %h:%p ops001
     StrictHostKeyChecking no

Host bastion bastion.banksimple.com
     ControlPath ~/.ssh/controls/ctl-%r-%n-%p
     ControlPersist 1h
     ProxyCommand   none
     LocalForward 8001 sensu.infra:443

Host *.dev *.pre *.prod *.corp *.infra *.part *.build *.dmz
     ProxyCommand ssh -W %h.banksimple.com:%p bastion
     StrictHostKeyChecking no

Host ip-*
     ProxyCommand ssh -W %h:%p bastion
     StrictHostKeyChecking no

Host *
    Compression     yes
    ConnectTimeout  200
    ControlPath     ~/.ssh/controls/ctl-%r-%h-%p
    ControlMaster   auto
    ForwardAgent    yes
    ForwardX11      no
    Protocol        2
    EscapeChar      !
    GlobalKnownHostsFile /dev/null
    HashKnownHosts  yes
    ServerAliveInterval 5
    User            will
    SendEnv         PS1 PROMPT_COMMAND
