Host devio.us
    User            wcmaier

Host openbsd
    HostName        cvs.openbsd.org
    User            wcmaier
    ForwardAgent    no
    LocalForward    7326 localhost:7326

Host meta
    HostName        meta.lfod.us
    RemoteForward   0 localhost:22
    SendEnv         SSH_*
    #RequestTTY     force

Host cloud
    HostName        cloud.lfod.us
    DynamicForward  31415

Host jumpbox.*
    Port            57783
    IdentitiesOnly  yes
    ProxyCommand    none
    IdentityFile    ~/.ssh/id_rsa

Host jumpbox.vpc
    HostName        50.112.135.97

Host jumpbox.ec2
    HostName        184.169.168.253

Host interactive
    HostName        interactive1
    ProxyCommand    /usr/local/bin/ssh jumpbox.ec2 nc %h %p

Host *.lfod.us
    User            will

Host ops001
    ProxyCommand    none

Host github.*
    ProxyCommand    none

Host *.amazonaws.com
    ProxyCommand    none

Host 127.0.0.1
    ProxyCommand    none
    ConnectTimeout  60

Host bastion bastion.banksimple.com
     ControlPath ~/.ssh/controls/ctl-%r-%n-%p
     ControlPersist 1h
     ProxyCommand   none

Host *.dev *.pre *.prod *.corp *.infra *.part *.build
     ProxyCommand ssh -W %h.banksimple.com:%p bastion

Host ip-*
     ProxyCommand ssh -W %h:%p bastion

Host *
    Compression     yes
    ConnectTimeout  200
    ControlPath     ~/.ssh/controls/ctl-%r-%h-%p
    ControlMaster   auto
    ForwardAgent    yes
    ForwardX11      no
    Protocol        2
    ProxyCommand    ssh ops001 nc %h %p
    EscapeChar      !
    GlobalKnownHostsFile /dev/null
    HashKnownHosts  yes
    ServerAliveInterval 5
    User            will
    SendEnv         PS1 PROMPT_COMMAND
