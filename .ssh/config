    ConnectTimeout  5
    ControlPath     ~/.ssh/controls/ctl-%r-%h-%p
    ControlMaster   auto
    #ControlPersist  1h
    ForwardAgent    yes
    ForwardX11      no
    Protocol        2
    ProxyCommand    ssh ops001 nc %h %p
    EscapeChar      !
    GlobalKnownHostsFile /dev/null
    HashKnownHosts  yes
    ServerAliveCountMax 3
    ServerAliveInterval 5
